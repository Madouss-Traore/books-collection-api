{
  "name": "books-collection-api",
  "version": "1.0.0",
  "description": "API REST pour gérer une collection de livres",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon index.js",
    "db:create": "psql -U madtae -d books -f ./data/create_tables.sql",
    "db:seed": "psql -U madtae -d books -f ./data/seeding_tables.sql",
    "db:reset": "npm run db:create && npm run db:seed",
    "db:test": "node test-seed.js",
    "test": "echo \"No tests configured yet\" && exit 0"
  },
  "keywords": ["books", "api", "node", "express", "postgresql", "sequelize"],
  "author": "Madouss Traoré",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "http-status-codes": "^2.3.0",
    "pg": "^8.16.3",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
